{
  "name": "otot-b",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:local:start": "docker compose up dev-db -d",
    "db:local:rm": "docker compose rm dev-db -s -f -v",
    "db:local:restart": "pnpm db:local:rm && pnpm db:local:start",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "build": "next build",
    "build:push": "pnpm prisma db push && pnpm build",
    "build:local": "pnpm db:local:start && sleep 10 && pnpm prisma:migrate:deploy && pnpm build",
    "dev": "next dev",
    "dev:local": "pnpm prisma:migrate:dev && next dev",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "start": "next start"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.3",
    "@hookform/resolvers": "^2.9.9",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^4.4.0",
    "@tanstack/react-query": "^4.10.0",
    "@trpc/client": "10.0.0-proxy-beta.17",
    "@trpc/next": "10.0.0-proxy-beta.17",
    "@trpc/react": "10.0.0-proxy-beta.17",
    "@trpc/server": "10.0.0-proxy-beta.17",
    "classnames": "^2.3.2",
    "cross-fetch": "^3.1.5",
    "next": "12.3.1",
    "next-auth": "^4.12.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.38.0",
    "superjson": "1.9.1",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "autoprefixer": "^10.4.7",
    "eslint": "8.22.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^4.4.0",
    "tailwindcss": "^3.1.6",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "ct3aMetadata": {
    "initVersion": "6.4.0"
  }
}
